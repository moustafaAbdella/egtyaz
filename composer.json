{
    "name": "tuandm/laravue",
    "description": "Ezuru Realstat Vue.js with The beautiful dashboard for Laravel built by VueJS.",
    "keywords": [
        "laravel",
        "laravue",
        "vue",
        "admin",
        "dashboard"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Momaiz",
        "email": "momaiz.net@gmail.com",
        "homepage": "https://momaiz.net",
        "role": "Developer"
    },
    {
        "name": "Gaber Abdo",
        "email": "futureslasher@gmail.com",
        "homepage": "https://momaiz.net",
        "role": "Developer"
    }],
    "type": "project",
    "require": {
        "php": "^8.1",
        "aloha/twilio": "^5.0",
        "fruitcake/php-cors": "^1.2",
        "doctrine/dbal": "^3.5",
        "laravel/framework": "^11.0",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "paytabscom/laravel_paytabs": "^1.9",
        "prettus/l5-repository": "^2.7",
        "sentry/sentry-laravel": "^4.5",
        "spatie/laravel-permission": "^6.0",
        "srmklive/paypal": "^3.0",
        "superbalist/laravel-zendesk": "^1.0",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/functions/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "EzuruCustom/core": {
            "type": "path",
            "url": "packages/ezurucustom/core",
            "options": {
                "symlink": true
            }
        },
        "Momaiz/LaravelPayfort": {
            "type": "path",
            "url": "packages/momaiz/laravelpayfort",
            "options": {
                "symlink": true
            }
        }
    }
}