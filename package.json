{
  "name": "laravue",
  "version": "0.4.0",
  "description": "A beautiful dashboard for Laravel built by VueJS",
  "author": "Tuan Duong <bacduong@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development BABEL_ENV=development NODE_OPTIONS=--openssl-legacy-provider node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint:fix": "eslint --fix resources/js/**/*.{js,vue}"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "axios": "^1.12.2",
    "babel-loader": "^10.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "core-js": "^3.46.0",
    "cross-env": "^5.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-webpack-plugin": "^4.1.0",
    "laravel-mix": "^6.0.49",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.93.2",
    "sass-loader": "^10.5.2",
    "script-loader": "^0.7.2",
    "svg-sprite-loader": "^3.2.6",
    "vue": "^2.7.16",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "clipboard": "^2.0.4",
    "driver.js": "^0.9.8",
    "dropzone": "^5.5.1",
    "echarts": "^6.0.0",
    "element-ui": "^2.15.14",
    "file-saver": "^2.0.1",
    "fuse.js": "^3.4.6",
    "js-cookie": "^2.2.1",
    "jsonlint": "^1.6.3",
    "jszip": "^3.2.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^3.2.0",
    "screenfull": "^4.2.1",
    "sortablejs": "^1.10.2",
    "tableexport": "^3.3.13",
    "tui-editor": "^1.4.10",
    "vue-authenticate": "^1.3.4",
    "vue-count-to": "^1.0.13",
    "vue-google-autocomplete": "^1.1.0",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.6.5",
    "vue-server-renderer": "^2.2.6",
    "vue-splitpane": "^1.0.6",
    "vue-star-rating": "^1.6.1",
    "vue2-google-maps": "^0.10.7",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.6.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "xlsx": "^0.18.5"
  }
}
